{
    "manifest_version": 3,
    "name": "CSV Matching Extension",
    "version": "1.0",
    "description": "Select a word and match with CSV data",
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}
